{
    "name": "agussuroyo/rate-limiter",
    "description": "A simple and flexible rate limiter for PHP",
    "type": "library",
    "license": "MIT",
    "version": "1.0.0",
    "autoload": {
        "psr-4": {
            "AgusSuroyo\\RateLimiter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpstan/phpstan": "^1.10",
        "predis/predis": "^2.3",
        "mockery/mockery": "^1.6"
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-with-coverage": "vendor/bin/phpunit --coverage-html coverage-report",
        "analyse": "phpstan analyse src"
    },
    "minimum-stability": "stable"
}
